    public static void reassign(Id projectId, Id newUserId, Id oldUserId, String comments) {
        try {
            CustomApprovalProcessControlsService.reassign(projectId, newUserId, oldUserId, comments);
        } catch (Exception e) {
            throw new AuraHandledException(e.getMessage());
        }
    }

    @AuraEnabled
    public static void addApprovers(Id projectId, String[] approverIds) {
        try {
            if (String.isBlank(approverIds[0])) { approverIds[0] = null; }
            for(Integer i=0; i<approverIds.size(); i++){
                if (String.isBlank(approverIds[i])) { approverIds[i] = approverIds[0]; }
            }
            CustomApprovalProcessControlsService.addApprovers(projectId, approverIds);
        } catch (Exception e) {
            throw new AuraHandledException(e.getMessage());
        }
    }

    @AuraEnabled
    public static List<Map<String, String>> getRejectionReasonPicklistValues() {
        List<Map<String, String>> result = new List<Map<String, String>>();
        List<Schema.PicklistEntry> picklistEntries = Project__c.RejectionReason__c.getDescribe().getPicklistValues();
        for (Schema.PicklistEntry picklistEntry : picklistEntries) {
            result.add(new Map<String, String> {
                'value' => picklistEntry.getValue(),
                'label' => picklistEntry.getLabel()
            });
        }
        return result;
    }

