    @AuraEnabled
    public static void massReassign(List<Map<String, String>> projectDelegations) {
        try {
            List<ProcessInstanceWorkitem> reassignments = new List<ProcessInstanceWorkitem>();
            List<Messaging.SingleEmailMessage> emails = new List<Messaging.SingleEmailMessage>();
            User user = [SELECT Id, PendingProjectsToReassign__c FROM User WHERE Id =: projectDelegations[0].get('oldUserId')];
            Map<String, Set<String>> projectIdToApprovals = user.PendingProjectsToReassign__c != null ? (Map<String, Set<String>>)(JSON.deserialize(user.PendingProjectsToReassign__c, Map<String, Set<String>>.class)) : new Map<String, Set<String>>();
            List<String> projectIds = new List<String>();
            for (Map<String, String> projectDelegation : projectDelegations) {
                projectIds.add(projectDelegation.get('projectId'));
            }
            Map<Id, Project__c> idToProject = new Map<Id, Project__c>(ProjectRepository.getProjectsByIds(projectIds));
            for (Map<String, String> projectDelegation : projectDelegations) {
                List<ProcessInstanceWorkitem> processInstanceWorkItems = CustomApprovalProcessControlsService.reassignWithReturn(idToProject.get(projectDelegation.get('projectId')), projectDelegation.get('newUserId'), projectDelegation.get('oldUserId'), projectDelegation.get('comments'), emails, false);
                if (!processInstanceWorkItems.isEmpty()) {
                    Set<String> approvals = new Set<String>();
                    for (ProcessInstanceWorkitem item : processInstanceWorkItems) {
                        reassignments.add(item);
                    if (projectIdToApprovals.containsKey(projectDelegation.get('projectId'))) {
                        approvals = projectIdToApprovals.get(projectDelegation.get('projectId'));
                            approvals.add(item.Id);
                    } else {
                            approvals.add(item.Id);
                        projectIdToApprovals.put(projectDelegation.get('projectId'), approvals);
                    }
                }
            }
            }
            if (!emails.isEmpty()) {
                Messaging.sendEmail(emails);
            }
            user.PendingProjectsToReassign__c = JSON.serialize(projectIdToApprovals);
            update user;
            update reassignments;
        } catch (Exception e) {
            throw new AuraHandledException(e.getMessage());
        }
    }

